{
  "name": "react-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "nth-check": ">=2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-deploy": "npm run build && npm run post-build-deploy",
    "post-build-deploy": "npm run mv-react-index && npm run rm-react-dev && npm run clean-docs && npm run cp-build",
    "mv-react-index": "mv -v build/index.html build/react.html",
    "rm-react-dev": "rm -Rv build/react-dev",
    "clean-docs": "rm -Rv ../../docs/static",
    "cp-build": "cp -Rv build/* ../../docs/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
